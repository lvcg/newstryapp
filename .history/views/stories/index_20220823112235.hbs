<h1>Stories</h1>
<div class="row">
    {{#each stories}}
        <div class="col s12 m6">
            <div class="card">
                <div class="card-image">
                    
                </div>
                <div class="card-content">
                    <span class="card-title">{{title}}</span>
                    <p>{{body}}</p>
            </div>
        </div> </div>
    {{/each}}
    {{else}}
        <p> No stories to show</p>
    {{/each}}
</div>