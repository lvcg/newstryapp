<h6>DashBoard</h6>
<h3>Welcome {{name}}</h3>
<p>Here are your stories</p>
{{#if stories}}
    <table class="striped">
        <thead>
            <tr>
                <th>Title</th>
                <th>Date</th>
                <th>Status</th>
                <th></th>
            </tr>
            
        </thead>
        <tbody>
            {{}}
        </tbody>
    </table>

{{else}}
    <p> You have not created any stories</p>
{{/if}}


